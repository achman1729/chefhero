<div class="index-footer">
    <% if user_signed_in? %>
        <p> <%= link_to "Profile", user_path(current_user)%> </p>
        <p> <%= link_to "Account", profile_path(current_user)%> </p>
        <p> <%= link_to "Order Histry", order_history_path(current_user)%> </p>
        <p> <%= link_to "Dashboard", dashboard_path if current_user.account_type == "chefhero" %> </p>
        <p> <%= link_to "Log Out", destroy_user_session_path, method: :delete %> </p>
    <% else %>
        <p> <%= link_to "Log In", new_user_session_path %> </p>
        <p> <%= link_to "Sign In", new_user_registration_path %> </p>
    <% end %>
    <p id="copy-text"> &copy Caleb Leung, Coder Academy 2020 </p>
</div>